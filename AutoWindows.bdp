Proc Mouse_PosFin:Entero[]
	Resultado = Mouse_Posici$F3n
	ReDim Preservar Resultado,4
	Resultado[2] = Resultado[0] - Matriz[0]
	Resultado[3] = Resultado[1] - Matriz[1]
	Resultado[0] = Matriz[0]
	Resultado[1] = Matriz[1]
FinProc

