Proc Clic_Izquierdo(Opcional Cantidad:Entero = 1,Opcional Velocidad:Entero = 0)
	Var Send:INPUT,i:Entero
	Send.Tipo = 0
	Send.Union.mi.dx = 0
	Send.Union.mi.dy = 0
	Contar i = 1 a Cantidad
		Send.Union.mi.dwFlags = &amp;0002
		   SendInput(1,Send@,Long(Send))
